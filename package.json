{
    "name": "FHiCL",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.34.0"
    },
    "publisher": "Mohamed Elashri",
    "description": "Fermilab Hierarchical Configuration Language Syntax",
    "repository": "https://github.com/MohamedElashri/VSCode-Language-Fhicl",
        "categories": [
            "Programming Languages",
            "Snippets"
        ],
    "contributes": {
        "languages": [
            {
                "id": "fcl",
                "aliases": [
                    "fcl",
                    "fichl",
                    "FHiCL"
                ],
                "extensions": [
                    ".fcl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "fcl",
                "scopeName": "source.fcl",
                "path": "./syntaxes/fcl.json"
            }
        ],
        "snippets": [
            {
                "language": "fcl",
                "path": "./syntaxes/snippets.json"
            }
        ]
    }
}