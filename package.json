{
    "name": "FHiCL",
    "displayName": "Fermilab FHiCL Language",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.34.0"
    },
    "publisher": "Mohamed Elashri",
    "description": "Fermilab Hierarchical Configuration Language Syntax",
    "repository": "https://github.com/MohamedElashri/VSCode-Language-Fhicl",
    "bugs": {
            "url": "https://github.com/MohamedElashri/VSCode-Language-Fhicl/issues"
        },
    "keywords": [
            "fcl",
            "FHiCL",
            "fermilab"
        ],    
    "categories": [
            "Programming Languages",
            "Snippets"
        ],
    "icon": "img/logo.png",    
    "contributes": {
        "languages": [
            {
                "id": "fcl",
                "aliases": [
                    "fcl",
                    "fichl",
                    "FHiCL"
                ],
                "extensions": [
                    ".fcl"
                ],
                "configuration": "./fcl_language-configuration.json"
            },
                        {
                            "id": "cfg",
                            "aliases": [
                                "JobSub"
                            ],
                            "extensions": [
                                ".cfg"
                            ],
                            "configuration": "./cfg_language-configuration.json"
                        }
        ],
        "grammars": [
            {
                "language": "fcl",
                "scopeName": "source.fcl",
                "path": "./syntaxes/fcl.json"
            },
            {
                    "language": "cfg",
                    "scopeName": "source.cfg",
                    "path": "./syntaxes/cfg.json"
            }
        ],
        "snippets": [
            {
                "language": "fcl",
                "path": "./syntaxes/snippets.json"
            }
        ]
    }
}